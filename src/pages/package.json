{
    "name": "user",
    "version": "1.0.0",
    "description": "User management",
    "author": "blueking",
    "scripts": {
        "server": "node build/prod-server.js",
        "dev": "better-npm-run dev",
        "clean": "rimraf dist",
        "clean-dll": "rimraf static/lib*",
        "build": "better-npm-run build",
        "build:analyzer": "better-npm-run build:analyzer",
        "dll": "better-npm-run dll",
        "lint": "eslint --fix --ext .js,.vue,.ts src",
        "style": "stylelint 'src/**/*.(vue|scss|css)' --fix"
    },
    "keywords": [],
    "license": "ISC",
    "betterScripts": {
        "dll": {
            "command": "babel-node build/build-dll.js"
        },
        "dev": {
            "command": "babel-node build/check-dll.js && babel-node build/dev-server.js",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "build": {
            "command": "npm run clean && npm run clean-dll && npm run dll && babel-node build/build.js",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "build:analyzer": {
            "command": "npm run clean && npm run clean-dll && npm run dll && babel-node build/build.js",
            "env": {
                "NODE_ENV": "production",
                "npm_config_report": 1
            }
        }
    },
